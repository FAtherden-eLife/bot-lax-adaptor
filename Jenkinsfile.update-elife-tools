def elifeToolsSummary;
elifeUpdatePipeline(
    { commit ->
        elifeToolsSummary = sh script: "./update-elife-tools.sh", returnStdout: true
        sh "git add requirements.txt"
    },
    {
        return "Updated elife-tools to: ${elifeToolsSummary}"
    },
    'update_elife_tools_',
    true
)
